{
  "name": "HTML5 Annotation Viewer",
  "version": "0.3",
  "manifest_version": 2,
  "description": "This extension enables to take a look into HTML5 annotations like microdata/RDFa-Lite",
  "icons": {"128": "icon.png"},
  "permissions": ["tabs", "https://people.csail.mit.edu/*"],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Annotation Viewer"}
}
